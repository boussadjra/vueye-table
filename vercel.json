{
    "version": 2,
    "buildCommand": "pnpm run docs:build",
    "framework": "vitepress",
    "installCommand": "pnpm install",
    "outputDirectory": ".vitepress/dist/",
    "public": true,
    "alias": "vueye-table-docs.vercel.app",
    "builds": [
        {
            "src": "docs/.vitepress/dist/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/docs/.vitepress/dist/$1"
        }
    ]
}